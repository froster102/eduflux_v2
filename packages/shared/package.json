{
  "name": "@eduflux-v2/shared",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "typecheck": "tsc --watch --noEmit",
    "lint": "eslint ./src --ext .ts --fix",
    "build": "tsc --build",
    "proto:gen": "grpc_tools_node_protoc  --plugin=protoc-gen-ts_proto=../../node_modules/.bin/protoc-gen-ts_proto  --ts_proto_out=./src/adapters/grpc/generated --ts_proto_opt=outputServices=grpc-js --proto_path=./src/adapters/grpc/proto  ./src/adapters/grpc/proto/*.proto"
  },
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dist/src/*.d.ts",
      "import": "./dist/src/*.js",
      "default": "./dist/src/*.js"
    }
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "ioredis": "^5.4.1"
  }
}
