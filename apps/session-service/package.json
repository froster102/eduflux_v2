{
  "name": "session-service",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/main.ts",
    "typecheck": "bun run tsc --watch --noEmit",
    "lint": "eslint ./src --ext .ts --fix",
    "build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile ./build/server ./src/main.ts",
    "proto:gen": "grpc_tools_node_protoc  --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto   --ts_proto_out=./src/api/grpc/generated  --ts_proto_opt=outputServices=grpc-js,snakeToCamel=true  --proto_path=./src/api/grpc/proto  ./src/api/grpc/proto/session.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "@internationalized/date": "^3.8.2",
    "elysia": "latest",
    "http-status": "^2.1.0",
    "inversify": "^7.0.0-alpha.5",
    "jose": "^6.0.12",
    "livekit-server-sdk": "^2.13.3",
    "luxon": "^3.7.1",
    "mongoose": "^8.16.4",
    "nanoid": "^5.1.5",
    "node-cron": "^4.2.1",
    "reflect-metadata": "^0.2.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^4.0.5",
    "amqplib": "^0.10.9",
    "ioredis": "^5.4.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/google-protobuf": "^3.15.12",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "bun-types": "latest",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.2.0",
    "grpc-tools": "^1.13.0",
    "ts-proto": "^2.7.3",
    "@types/amqplib": "^0.10.8",
    "@types/luxon": "^3.6.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "module": "src/main.js"
}
